   "source": [
    "unique_emails = {}\n",
    "for email, date in trans:\n",
    "    if email not in unique_emails or date < unique_emails[email]:\n",
    "        unique_emails[email] = date\n",
    "\n",
    "result = [[email, date] for email, date in unique_emails.items()]\n",
    "print(result)"
   ]
  },
